
((TESTY::NAME . "ADD-TAGS-1") (TESTY::FILE-ON-DISK . "ADD-TAGS-1.test")
 (TESTY::DESCRIPTION
  . "Test that add tags successfully adds the additional tags 'soot' and 'newt' to a test originally defined only with the tags 'poot' and 'woot'")
 (TESTY::EXPECTATION . "EQUALP") (TESTY::TAGS)
 (TESTY::SOURCE LAMBDA NIL
                (LET ((TESTY::*TEST-NAMES* (MAKE-HASH-TABLE :TEST #'EQUALP))
                      (TESTY::*TEST-TAGS* (MAKE-HASH-TABLE :TEST #'EQUALP)))
                  (TESTY:TEST :NAME "a-test" :SOURCE (+ 1 1) :TAGS
                              '("woot" "poot") :EXPECTED-VALUE 2)
                  (TESTY::ADD-TAGS "a-test" '("soot" "newt"))
                  (TESTY::GET-TAGS "a-test")))
 (TESTY::RE-EVALUATE) (TESTY::EXPECTED-VALUE "NEWT" "SOOT" "WOOT" "POOT")
 (TESTY::RUN-VALUE "NEWT" "SOOT" "WOOT" "POOT") (TESTY::RUN-TIME . 0.0)
 (TESTY::RESULT . T) (TESTY::BEFORE-FUNCTION-SOURCE LAMBDA NIL NIL)
 (TESTY::BEFORE-FUNCTION-RUN-STATUS . T)
 (TESTY::AFTER-FUNCTION-SOURCE LAMBDA NIL NIL)
 (TESTY::AFTER-FUNCTION-RUN-STATUS . T)) 