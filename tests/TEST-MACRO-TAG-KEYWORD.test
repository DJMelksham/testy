
((NAME . "TEST-MACRO-TAG-KEYWORD")
 (FILE-ON-DISK . "TEST-MACRO-TAG-KEYWORD.test")
 (DESCRIPTION
  . "Does the :tag keyword supplied as an argument to (test) properly register the tag 'woot' in *test-tags*")
 (EXPECTATION . "EQUALP") (TAGS)
 (SOURCE LAMBDA NIL
         (LET ((*TEST-NAMES* (MAKE-HASH-TABLE :TEST #'EQUALP))
               (*TEST-TAGS* (MAKE-HASH-TABLE :TEST #'EQUALP)))
           (TEST :SOURCE (+ 1 1) :TAGS "woot" :EXPECTED-VALUE 2)
           (TEST :SOURCE (+ 1 2) :EXPECTED-VALUE 3)
           (TEST :SOURCE (+ 1 3) :EXPECTED-VALUE 4)
           (LOOP FOR MEH BEING THE HASH-KEYS IN *TEST-TAGS*
                 COLLECT MEH)))
 (RE-EVALUATE) (EXPECTED-VALUE "WOOT") (RUN-VALUE "WOOT") (RUN-TIME . 0.002)
 (RESULT . T) (BEFORE-FUNCTION-SOURCE LAMBDA NIL NIL)
 (BEFORE-FUNCTION-RUN-STATUS . T) (AFTER-FUNCTION-SOURCE LAMBDA NIL NIL)
 (AFTER-FUNCTION-RUN-STATUS . T) (TYPE-OF-TEST)) 