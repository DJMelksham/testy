
((TESTY::NAME . "TEST-MACRO-TAG-KEYWORD")
 (TESTY::FILE-ON-DISK . "TEST-MACRO-TAG-KEYWORD.test")
 (TESTY::DESCRIPTION
  . "Does the :tag keyword supplied as an argument to (test) properly register the tag 'woot' in *test-tags*")
 (TESTY::EXPECTATION . "EQUALP") (TESTY::TAGS)
 (TESTY::SOURCE LAMBDA NIL
                (LET ((TESTY::*TEST-NAMES* (MAKE-HASH-TABLE :TEST #'EQUALP))
                      (TESTY::*TEST-TAGS* (MAKE-HASH-TABLE :TEST #'EQUALP)))
                  (TESTY:TEST :SOURCE (+ 1 1) :TAGS "woot" :EXPECTED-VALUE 2)
                  (TESTY:TEST :SOURCE (+ 1 2) :EXPECTED-VALUE 3)
                  (TESTY:TEST :SOURCE (+ 1 3) :EXPECTED-VALUE 4)
                  (LOOP TESTY::FOR TESTY::MEH TESTY::BEING THE TESTY::HASH-KEYS TESTY::IN TESTY::*TEST-TAGS*
                        TESTY::COLLECT TESTY::MEH)))
 (TESTY::RE-EVALUATE) (TESTY::EXPECTED-VALUE "WOOT") (TESTY::RUN-VALUE "WOOT")
 (TESTY::RUN-TIME . 0.002) (TESTY::RESULT . T)
 (TESTY::BEFORE-FUNCTION-SOURCE LAMBDA NIL NIL)
 (TESTY::BEFORE-FUNCTION-RUN-STATUS . T)
 (TESTY::AFTER-FUNCTION-SOURCE LAMBDA NIL NIL)
 (TESTY::AFTER-FUNCTION-RUN-STATUS . T)) 