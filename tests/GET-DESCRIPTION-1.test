
((TESTY::NAME . "GET-DESCRIPTION-1")
 (TESTY::FILE-ON-DISK . "GET-DESCRIPTION-1.test")
 (TESTY::DESCRIPTION
  . "Testing whether we can get the description of a test referencing it by name and feeding it to the (get-description) function")
 (TESTY::EXPECTATION . "EQUALP") (TESTY::TAGS)
 (TESTY::SOURCE LAMBDA NIL
                (LET ((TESTY::*TEST-NAMES* (MAKE-HASH-TABLE :TEST #'EQUALP))
                      (TESTY::*TEST-TAGS* (MAKE-HASH-TABLE :TEST #'EQUALP)))
                  (TESTY:TEST :NAME "a-test" :DESCRIPTION "whackadoo" :SOURCE
                              (+ 1 1) :TAGS '("woot" "poot") :EXPECTED-VALUE 2)
                  (TESTY::GET-DESCRIPTION "a-test")))
 (TESTY::RE-EVALUATE) (TESTY::EXPECTED-VALUE . "whackadoo")
 (TESTY::RUN-VALUE . "whackadoo") (TESTY::RUN-TIME . 0.001) (TESTY::RESULT . T)
 (TESTY::BEFORE-FUNCTION-SOURCE LAMBDA NIL NIL)
 (TESTY::BEFORE-FUNCTION-RUN-STATUS . T)
 (TESTY::AFTER-FUNCTION-SOURCE LAMBDA NIL NIL)
 (TESTY::AFTER-FUNCTION-RUN-STATUS . T)) 