
((TESTY::NAME . "GET-TESTS-1") (TESTY::FILE-ON-DISK . "GET-TESTS-1.test")
 (TESTY::DESCRIPTION
  . "Verifies that (get-tests) by default returns all of the tests that have been registered, and that all the items in the sequence are tests")
 (TESTY::EXPECTATION . "EQ") (TESTY::TAGS)
 (TESTY::SOURCE LAMBDA NIL
                (LET ((TESTY::*TEST-NAMES* (MAKE-HASH-TABLE :TEST #'EQUALP)))
                  (TESTY:TEST :SOURCE (+ 1 1) :EXPECTED-VALUE 2)
                  (TESTY:TEST :SOURCE (+ 1 2) :EXPECTED-VALUE 3)
                  (TESTY:TEST :SOURCE (+ 1 3) :EXPECTED-VALUE 4)
                  (AND
                   (EVERY (LAMBDA (TESTY::X) (TYPEP TESTY::X 'TESTY:TEST))
                          (TESTY:GET-TESTS))
                   (= (LENGTH (TESTY:GET-TESTS)) 3))))
 (TESTY::RE-EVALUATE) (TESTY::EXPECTED-VALUE . T) (TESTY::RUN-VALUE . T)
 (TESTY::RUN-TIME . 0.003) (TESTY::RESULT . T)
 (TESTY::BEFORE-FUNCTION-SOURCE LAMBDA NIL NIL)
 (TESTY::BEFORE-FUNCTION-RUN-STATUS . T)
 (TESTY::AFTER-FUNCTION-SOURCE LAMBDA NIL NIL)
 (TESTY::AFTER-FUNCTION-RUN-STATUS . T)) 